@model IEnumerable<Blog.DAL.Entity.ORM.Entity.Concrete.Category>


    <div class="well">
        <h4>Blog Kategorileri</h4>
        <hr>
        <div class="row">

            @foreach (var item in Model)
            {
                <div class="col-lg-6">
                    <ul class="list-unstyled">
                        <li>
                            <a href="/Posts/List/@item.ID">@item.CategoryName</a>
                        </li>
                    </ul>
                </div>
            }


        </div>
        <!-- /.row -->
    </div>